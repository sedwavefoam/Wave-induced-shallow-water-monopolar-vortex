FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      fmm;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 4 -4 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    backWall
    {
        type            zeroGradient;
    }

    sideWallS
    {
        type            zeroGradient;
    }
    sideWallNS
    {
        type            zeroGradient;
    }
    breakwater
    {
        type            zeroGradient;
    }
    lowerWallS
    {
        type            zeroGradient;
    }
	lowerWallNS
    {
        type            zeroGradient;
    }
    atmosphereS
    {
        type            zeroGradient;
    }
	atmosphereNS
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}

